@{
    ViewData["Title"] = "Cablemods No verificados";
}

<h1>Listado de cablemodems no verificados</h1>
<br />
<partial name="_Buscar" />
<br />
@if (ViewBag.Error is null)
{
    <h4 class="text-black-50">Se ha agregado el modelo a los verificados</h4>
    <br />
    @if (ViewBag.CablemodemsCount == 0)
    {
        <h4 class="text-black-50">No se han encontrado cablemodems que no esten registrados en la lista de modelos</h4>
    }
    else
    {
        <partial name="_Modelos" />
    }
}
else
{
    <h6 class="text-black-50">@ViewBag.Error</h6>
}